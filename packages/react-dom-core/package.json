{
  "name": "react-dom-core",
  "version": "0.1.2",
  "description": "Copy of react-dom 15",
  "scripts": {
    "build": "npm run copy && npm run replace",
    "clean": "rm -rf dist lib index.js server.js test-utils.js",
    "copy": "cd node_modules/react-dom && cp -r dist lib index.js server.js test-utils.js ../..",
    "prepublishOnly": "npm run clean && npm run build",
    "replace": "scripts/replace.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/react-dom-core"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/react-dom-core/issues"
  },
  "keywords": [
    "react",
    "react-15",
    "react-dom",
    "react-dom-core"
  ],
  "files": [
    "dist",
    "lib",
    "index.js",
    "server.js",
    "test-utils.js"
  ],
  "dependencies": {
    "fbjs": "1.0.0",
    "object-assign": "4.1.1",
    "react-15": "0.2.0"
  },
  "devDependencies": {
    "react-dom": "^18"
  },
  "license": "MIT"
}

{
  "name": "react-property",
  "version": "2.0.0",
  "description": "HTML and SVG DOM property configs used by React.",
  "main": "lib/index.js",
  "scripts": {
    "build": "rollup -c rollup.config.possibleStandardNames.mjs && scripts/build.js && rollup -c",
    "clean": "rm -rf lib",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm run clean && npm run build && npm test",
    "test": "jest --coverage --ci",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/react-dom-core"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/react-dom-core/issues"
  },
  "keywords": [
    "react-property",
    "html",
    "svg",
    "dom",
    "property",
    "attribute",
    "config",
    "react",
    "react-dom"
  ],
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-flow": "^7.22.15",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "eslint": "^8.52.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "rollup": "^4.1.4"
  },
  "license": "MIT"
}

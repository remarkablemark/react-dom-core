{
  "name": "react-property",
  "version": "2.0.2",
  "description": "HTML and SVG DOM property configs used by React.",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && rollup -c rollup.config.possibleStandardNames.mjs && scripts/build.js && rollup -c && tsc lib/*.js --declaration --allowJs --emitDeclarationOnly",
    "clean": "rm -rf lib",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run lint && npm run build && npm test",
    "test": "jest --coverage --ci",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remarkablemark/react-dom-core"
  },
  "bugs": {
    "url": "https://github.com/remarkablemark/react-dom-core/issues"
  },
  "keywords": [
    "react-property",
    "html",
    "svg",
    "dom",
    "property",
    "attribute",
    "config",
    "react",
    "react-dom"
  ],
  "files": [
    "/lib"
  ],
  "devDependencies": {
    "@eslint/compat": "2.0.2",
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@rollup/plugin-alias": "6.0.0",
    "@rollup/plugin-sucrase": "5.0.2",
    "eslint": "9.39.2",
    "eslint-plugin-ft-flow": "3.0.11",
    "eslint-plugin-prettier": "5.5.5",
    "globals": "17.2.0",
    "hermes-eslint": "0.33.3",
    "jest": "30.2.0",
    "prettier": "3.8.1",
    "rollup": "4.57.0",
    "typescript": "5.9.3"
  },
  "license": "MIT"
}
